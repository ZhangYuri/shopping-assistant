{
    "name": "shopping-assistant",
    "version": "1.0.0",
    "description": "Multi-agent shopping assistant system using LangChain and LangGraph",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:pbt": "jest --testNamePattern=\"Property\"",
        "test:db": "node scripts/test-database-integration.js",
        "setup:ocr": "node scripts/setup-ocr-data.js",
        "check:ocr": "node scripts/check-traineddata-status.js",
        "setup:env": "node scripts/setup-env.js",
        "test:db-connection": "node scripts/test-db-connection.js",
        "check:env": "node scripts/check-env.js",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "clean": "rimraf dist",
        "format": "prettier --write \"src/**/*.{ts,js,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
        "docker:setup:dev": "docker/scripts/docker-setup.sh setup dev",
        "docker:setup:prod": "docker/scripts/docker-setup.sh setup prod",
        "docker:start:dev": "docker-compose -f docker-compose.dev.yml up --build -d",
        "docker:start:prod": "docker-compose up --build -d",
        "docker:stop:dev": "docker-compose -f docker-compose.dev.yml down",
        "docker:stop:prod": "docker-compose down",
        "docker:logs:dev": "docker-compose -f docker-compose.dev.yml logs -f",
        "docker:logs:prod": "docker-compose logs -f",
        "docker:migrate": "node scripts/run-migrations.js",
        "docker:validate-migrations": "node scripts/validate-migrations.js",
        "docker:cleanup": "docker-compose down -v && docker system prune -f"
    },
    "author": "Yuri",
    "license": "ISC",
    "dependencies": {
        "@langchain/community": "^0.3.0",
        "@langchain/core": "^1.0.0",
        "@langchain/deepseek": "^1.0.2",
        "@langchain/langgraph": "^0.2.0",
        "@langchain/mcp-adapters": "^1.1.0",
        "dotenv": "^16.4.0",
        "express": "^5.2.1",
        "joi": "^17.12.0",
        "langchain": "^1.2.0",
        "multer": "^2.0.2",
        "mysql2": "^3.15.3",
        "sharp": "^0.34.5",
        "tesseract.js": "^6.0.1",
        "uuid": "^10.0.0",
        "winston": "^3.11.0",
        "xlsx": "^0.18.5",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/multer": "^2.0.0",
        "@types/node": "^24.10.1",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.57.0",
        "fast-check": "^3.15.0",
        "jest": "^29.7.0",
        "prettier": "^3.7.4",
        "rimraf": "^5.0.0",
        "supertest": "^7.1.4",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.0",
        "typescript": "^5.3.0"
    }
}
